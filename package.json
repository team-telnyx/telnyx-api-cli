{
  "name": "@telnyx/api-cli",
  "description": "Command-line interface for Telnyx APIs",
  "version": "1.0.0",
  "author": "Telnyx <support@telnyx.com>",
  "bin": {
    "telnyx": "./bin/run.js"
  },
  "bugs": "https://github.com/team-telnyx/telnyx-api-cli/issues",
  "homepage": "https://github.com/team-telnyx/telnyx-api-cli",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/team-telnyx/telnyx-api-cli.git"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.978.0",
    "@inquirer/prompts": "^8.2.0",
    "@oclif/core": "^4",
    "@oclif/plugin-autocomplete": "^3",
    "@oclif/plugin-help": "^6",
    "telnyx": "^2"
  },
  "devDependencies": {
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^4",
    "@types/chai": "^5.2.3",
    "@types/mocha": "^10.0.10",
    "@types/node": "^18",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "c8": "^10.1.3",
    "chai": "^6.2.2",
    "eslint": "^9.39.2",
    "mocha": "^10",
    "oclif": "^4",
    "shx": "^0.3.3",
    "ts-node": "^10",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "files": [
    "./bin",
    "./dist",
    "./oclif.manifest.json"
  ],
  "keywords": [
    "telnyx",
    "cli",
    "10dlc",
    "sms",
    "mms",
    "messaging",
    "voice",
    "voip",
    "telecom",
    "phone-numbers",
    "verification",
    "2fa",
    "fax",
    "video",
    "storage",
    "ai",
    "llm",
    "iot",
    "sim"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "type": "module",
  "oclif": {
    "bin": "telnyx",
    "dirname": "telnyx",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-autocomplete"
    ],
    "topicSeparator": " ",
    "topics": {
      "auth": {
        "description": "Manage API authentication"
      },
      "10dlc": {
        "description": "10DLC brand and campaign registration"
      },
      "10dlc:brand": {
        "description": "Manage 10DLC brands"
      },
      "10dlc:campaign": {
        "description": "Manage 10DLC campaigns"
      },
      "ai": {
        "description": "AI inference (chat, embeddings)"
      },
      "assistant": {
        "description": "AI voice assistants"
      },
      "billing": {
        "description": "Account balance and billing groups"
      },
      "billing:group": {
        "description": "Manage billing groups"
      },
      "call": {
        "description": "Voice call control"
      },
      "connection": {
        "description": "Voice connections and applications"
      },
      "e911": {
        "description": "E911 emergency services"
      },
      "e911:address": {
        "description": "Emergency addresses"
      },
      "fax": {
        "description": "Send and receive faxes"
      },
      "lookup": {
        "description": "Phone number lookup"
      },
      "message": {
        "description": "Send and manage SMS/MMS messages"
      },
      "messaging-profile": {
        "description": "Messaging profile configuration"
      },
      "number": {
        "description": "Phone number management"
      },
      "porting": {
        "description": "Number porting"
      },
      "porting:order": {
        "description": "Port-in orders"
      },
      "profile": {
        "description": "Manage CLI profiles"
      },
      "sim": {
        "description": "IoT SIM card management"
      },
      "storage": {
        "description": "Cloud storage (S3-compatible)"
      },
      "storage:bucket": {
        "description": "Storage buckets"
      },
      "storage:object": {
        "description": "Storage objects"
      },
      "usage": {
        "description": "Usage reports and analytics"
      },
      "verify": {
        "description": "Phone verification (2FA)"
      },
      "verify:profile": {
        "description": "Verify profiles"
      },
      "verify:template": {
        "description": "Verification message templates"
      },
      "video": {
        "description": "Video rooms and conferencing"
      },
      "video:room": {
        "description": "Video room management"
      },
      "video:session": {
        "description": "Video sessions"
      },
      "video:recording": {
        "description": "Video recordings"
      },
      "voice-profile": {
        "description": "Outbound voice profiles"
      }
    }
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b && oclif manifest",
    "lint": "eslint",
    "postpack": "shx rm -f oclif.manifest.json",
    "prepack": "oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "test:coverage": "c8 mocha --forbid-only \"test/**/*.test.ts\"",
    "test:ci": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "types": "dist/index.d.ts"
}
